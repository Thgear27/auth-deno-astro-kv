{
  "name": "auth-deno-kv",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "deno run -A --env-file=.env --unstable-kv npm:astro dev",
    "build": "astro build",
    "preview": "deno run --allow-net --allow-read --unstable-kv --allow-env ./dist/server/entry.mjs",
    "astro": "astro",
    "test": "deno run -A --unstable-kv npm:vitest",
    "generate:types:deno": "deno types > lib.deno.d.ts",
    "check:format": "prettier --check --plugin prettier-plugin-astro .",
    "format": "prettier --write --plugin prettier-plugin-astro ."
  },
  "dependencies": {
    "@astrojs/deno": "5.0.1",
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "1.1.0",
    "arctic": "3.6.0",
    "astro": "5.7.4",
    "prettier": "3.5.3",
    "prettier-plugin-astro": "0.14.1",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "vitest": "3.1.2"
  }
}
